{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "acshb",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "acshb",
      "kebab-case"
    ]
    ,
    "jsdoc-format": false,
    "no-redundant-jsdoc": false,
    "max-line-length": [true, 600]
  }
}
